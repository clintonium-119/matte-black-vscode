    },
    {
      "scope": "strong",
      "settings": { "fontStyle": "bold" }
    },

    // Comments
    {
      "scope": [
        "comment",
        "comment.block",
        "comment.line",
        "punctuation.definition.comment"
      ],
      "settings": { "foreground": "#8A8A8D", "fontStyle": "italic" }
    },

    // Strings & regex & escapes
    {
      "scope": ["string", "string.quoted", "string.unquoted"],
      "settings": { "foreground": "#eaeaea" }
    },
    {
      "scope": ["string.regexp", "string.regex"],
      "settings": { "foreground": "#D97706" }
    },
    {
      "scope": [
        "constant.character.escape",
        "string.escape",
        "string.special",
        "string.special.symbol"
      ],
      "settings": { "foreground": "#EFBF04" }
    },

    // Numbers & booleans
    {
      "scope": ["constant.numeric", "number", "float"],
      "settings": { "foreground": "#EFBF04" }
    },
    {
      "scope": [
        "constant.language.boolean",
        "constant.language.true",
        "constant.language.false"
      ],
      "settings": { "foreground": "#10B981" }
    },

    // Variables
    {
      "scope": ["variable", "variable.other.readwrite"],
      "settings": { "foreground": "#D97706" }
    },
    {
      "scope": [
        "variable.language",
        "variable.language.this",
        "variable.builtin"
      ],
      "settings": { "foreground": "#3B82F6" }
    },
    {
      "scope": "variable.parameter",
      "settings": { "foreground": "#bebebe" }
    },
    {
      "scope": "variable.other.member",
      "settings": { "foreground": "#eaeaea" }
    },

    // Constants & macros
    {
      "scope": ["constant", "constant.other"],
      "settings": { "foreground": "#D97706" }
    },
    {
      "scope": ["meta.preprocessor", "entity.name.function.preprocessor"],
      "settings": { "foreground": "#FBBF24" }
    },

    // Keywords & operators
    {
      "scope": ["keyword", "keyword.control", "keyword.type", "storage.type"],
      "settings": { "foreground": "#059669" }
    },
    {
      "scope": ["keyword.operator", "operator"],
      "settings": { "foreground": "#bebebe" }
    },

    // Properties / object keys
    {
      "scope": [
        "variable.other.property",
        "support.type.property-name",
        "meta.object-literal.key"
      ],
      "settings": { "foreground": "#F59E0B" }
    },

    // Functions (decl vs call)
    {
      "scope": ["entity.name.function", "entity.name.function.constructor"],
      "settings": { "foreground": "#DC2626" }
    },
    {
      "scope": [
        "meta.function-call",
        "meta.method-call",
        "support.function",
        "entity.name.function.call",
        "entity.name.function.method",
        "variable.function"
      ],
      "settings": { "foreground": "#F59E0B" }
    },

    // Types & classes
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class",
        "support.type"
      ],
      "settings": { "foreground": "#FBBF24" }
    },
    {
      "scope": ["support.type.builtin", "storage.type.builtin"],
      "settings": { "foreground": "#F59E0B", "fontStyle": "italic" }
    },
    {
      "scope": ["entity.name.namespace", "support.module", "namespace"],
      "settings": { "foreground": "#BF9903", "fontStyle": "italic" }
    },

    // Decorators / annotations
    {
      "scope": [
        "meta.decorator",
        "entity.name.function.decorator",
        "punctuation.decorator"
      ],
      "settings": { "foreground": "#D97706" }
    },

    // HTML/XML tags & attributes
    {
      "scope": ["entity.name.tag", "tag"],
      "settings": { "foreground": "#059669" }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#D97706", "fontStyle": "italic" }
    },
    {
      "name": "Tag delimiters",
      "scope": ["punctuation.definition.tag"],
      "settings": { "foreground": "#bebebe" }
    },

    // Punctuation
    {
      "scope": [
        "punctuation",
        "punctuation.separator",
        "punctuation.accessor",
        "punctuation.bracket"
      ],
      "settings": { "foreground": "#8A8A8D" }
    },
    {
      "scope": ["punctuation.special", "punctuation.special.symbol"],
      "settings": { "foreground": "#3B82F6" }
    },

    // Diff/SCM
    {
      "scope": "markup.inserted",
      "settings": { "foreground": "#10B981" }
    },
    {
      "scope": "markup.deleted",
      "settings": { "foreground": "#DC2626" }
    },
    {
      "scope": "markup.changed",
      "settings": { "foreground": "#F59E0B" }
    },

    // Markdown / text
    {
      "scope": "markup.heading",
      "settings": { "foreground": "#D97706", "fontStyle": "bold" }
    },
    {
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": { "foreground": "#F59E0B" }
    }
  ],
  // Semantic token colors for precise identifier styling
  "semanticTokenColors": {
    // Imported bindings (read-only declarations)
    "variable.declaration.readonly": "#EFBF04",
    // General read-only variables (includes imports and const)
    "variable.readonly": "#EFBF04",
    // let/var declarations (non-readonly)
    "variable.declaration": "#D97706"
  }
}